{
    "name": "swoolecan/foundation",
    "description": "PHP基础代码",
    "keywords": [
        "php",
        "hyperf",
        "foundation",
        "swoole",
        "laravel"
    ],
    "homepage": "https://github.com/swoolecan/foundation",
    "license": "MIT",
    "authors": [
        {
            "name": "wangcan",
            "homepage": "https://github.com/swoolecan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "jenssegers/agent": "^2.6",
        "overtrue/easy-sms": "^3.0",
        "w7corp/easywechat": "^5.35"
    },
    "autoload": {
        "psr-4": {
            "Swoolecan\\Foundation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swoolecan\\Foundation\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
